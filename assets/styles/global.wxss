.page,
body {
  min-height: 100vh
}

.page {
  font-size: 16px;
  line-height: 1.6
}

.page,
body {
  background: #efeff4;
  color: #000
}

checkbox,
radio {
  margin-right: 5px
}

button {
  margin-bottom: 10px;
  margin-top: 10px
}

form {
  width: 100%
}

.pos-r {
  position: relative
}

.loading-mask {
  background: #fff;
  height: 100vh
}

.loading-mask,
.share-mask {
  left: 0;
  position: fixed;
  top: 0;
  width: 100%
}

.share-mask {
  background: rgba(0, 0, 0, .8);
  color: #fff;
  height: 100%
}

.share-mask image {
  position: absolute;
  right: 78px;
  top: 6px
}

.share-mask.ios image {
  right: 63px
}

.go-desktop-prompt {
  height: 100%;
  left: 0;
  position: fixed;
  top: 0;
  width: 100%
}

.go-desktop-prompt .mask {
  background: rgba(0, 0, 0, .6);
  bottom: 0;
  left: 0;
  position: fixed;
  right: 0;
  top: 0;
  z-index: 1000
}

.go-desktop-prompt .dialog-container {
  height: 315px;
  margin: 50px auto 0;
  padding: 0 25px;
  position: relative;
  z-index: 1001
}

.go-desktop-prompt .dialog {
  background-color: #fff;
  border-radius: 6px;
  font-size: 0;
  padding: 35px 15px 25px;
  position: relative
}

.go-desktop-prompt .dialog .desktop-img {
  height: 116px;
  margin: 0 22px 20px;
  width: 252px
}

.go-desktop-prompt .dialog .btn-area {
  margin-top: 25px;
  padding: 0 5px
}

.go-desktop-prompt .dialog .confirm-btn {
  background: #019aff;
  color: #fff
}

.go-desktop-prompt .dialog .prompt-text {
  font-size: 16px;
  line-height: 22px;
  text-align: center
}

.go-desktop-prompt .dialog .close {
  height: 10px;
  position: absolute;
  right: 20px;
  top: 20px;
  width: 10px
}

.go-desktop-prompt .dialog .close image {
  width: 100%
}

.status-bar {
  background: #fff;
  margin: 10px 0;
  padding: 10px 0
}

.status-box {
  background: #fff;
  color: #737373;
  font-size: 14px;
  line-height: 1.2;
  margin-top: 10px;
  padding: 17px 0;
  text-align: center
}

.status-box .weui-flex__item+.weui-flex__item {
  position: relative
}

.status-box .weui-flex__item+.weui-flex__item::before {
  background: #e5e5e5;
  content: "";
  display: block;
  height: 26px;
  left: 0;
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  width: 1px
}

.status-box .num {
  color: #000;
  font-size: 26px
}

.fixed-bottom {
  background: #fff;
  bottom: 0;
  box-sizing: border-box;
  left: 0;
  position: fixed;
  width: 100%;
  z-index: 1
}

.fixed-btns {
  line-height: 3em;
  text-align: center;
  z-index: 10
}

.fixed-btns .weui-flex__item+.weui-flex__item {
  border-left: 1px solid #eeeef3
}

.full-fallback {
  -webkit-align-items: center;
  align-items: center;
  background: #fff;
  -webkit-flex-direction: column;
  flex-direction: column;
  height: 100vh;
  -webkit-justify-content: center;
  justify-content: center
}

.full-fallback image {
  border: none;
  height: 191px;
  margin-bottom: 30px;
  width: 194px
}

.full-fallback .title {
  color: var(--grey-black);
  font-size: 20px;
  margin-bottom: 8px
}

.full-fallback .tips {
  font-size: 14px;
  padding: 0 10%;
  text-align: center
}

.warning {
  background: #f43530;
  color: #fff;
  font-size: 15px;
  line-height: 20px;
  padding: 10px 5px;
  text-align: center
}

.modal {
  top: 20%
}

.modal,
.modal-bg {
  position: fixed;
  width: 100%;
  z-index: 1000
}

.modal-bg {
  background: hsla(0, 0%, 59%, .9);
  height: 100%;
  left: 0;
  top: 0
}

.btn-upgrade {
  background-color: #2875e8;
  color: #fff
}

.btn-cancel,
.btn-upgrade {
  border-radius: 2px;
  line-height: 2;
  margin: 7px 0
}

.btn-cancel {
  color: #000
}

.btn-contact {
  background-color: #2875e8;
  border-radius: 2px;
  color: #fff;
  line-height: 2;
  margin: 7px 0
}

